local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("RomaGay Hub", "DarkTheme")
local Tab = Window:NewTab("Functions")
local Section = Tab:NewSection("Roma")
Section:NewButton("Make Roma gay", "ButtonInfo", function()
    Wait(3)
    print("Loading...")
    Wait(5)
    print("Successfully!")
    Wait(1)
    print("Roma is now gay!!!")
end)
Section:NewSlider("How many stepfathers will Roma have?", "SliderInfo", 10, 0, function(s)
    Section:NewButton("Do stepfathers", "ButtonInfo", function ()
        print(s)
    end)
end)
